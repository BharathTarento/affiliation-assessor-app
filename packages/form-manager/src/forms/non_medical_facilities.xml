            </instance>
            <bind nodeset="/data/part1_4_1/teaching_block" type="string"/>
            <bind nodeset="/data/part1_4_1/why86" relevant=" /data/part1_4_1/teaching_block  = 'no'" type="string"/>
            <bind nodeset="/data/part1_4_1/image1" type="binary"/>
            <bind nodeset="/data/part1_4_1/classrooms" type="int"/>
            <bind nodeset="/data/part1_4_1/why87" relevant=" /data/part1_4_1/classrooms  = 'no'" type="string"/>
            <bind nodeset="/data/part1_4_1/image2" type="binary"/>
            <bind nodeset="/data/part1_4_1/enough_space" type="int"/>
            <bind nodeset="/data/part1_4_1/why88" relevant=" /data/part1_4_1/enough_space  = 'no'" type="string"/>
            <bind nodeset="/data/part1_4_1/image3" type="binary"/>
            <bind nodeset="/data/part1_4_1/principal_office" type="string"/>
            <bind nodeset="/data/part1_4_1/why89" relevant=" /data/part1_4_1/principal_office  = 'no'" type="string"/>
            <bind nodeset="/data/part1_4_1/image4" type="binary"/>
            <bind nodeset="/data/part1_4_1/teaching_fac" type="string"/>
            <bind nodeset="/data/part1_4_1/why90" relevant=" /data/part1_4_1/teaching_fac  = 'no'" type="string"/>
            <bind nodeset="/data/part1_4_1/image5" type="binary"/>
            <bind nodeset="/data/part1_4_1/staff" type="string"/>
            <bind nodeset="/data/part1_4_1/why91" relevant=" /data/part1_4_1/staff  = 'no'" type="string"/>
            <bind nodeset="/data/part1_4_1/image6" type="binary"/>
            <bind nodeset="/data/part1_4_3/library_comp" type="string"/>
            <bind nodeset="/data/part1_4_3/why98" relevant=" /data/part1_4_3/library_comp  = 'no'" type="string"/>
            <bind nodeset="/data/part1_4_3/image7" type="binary"/>
            <bind nodeset="/data/part1_4_3/books" type="int"/>
            <bind nodeset="/data/part1_4_3/journal" type="int"/>
            <bind nodeset="/data/part1_4_3/library_seating" type="int"/>
            <bind nodeset="/data/part1_4_3/util_register" type="string"/>
            <bind nodeset="/data/part1_4_3/why102" relevant=" /data/part1_4_3/util_register  = 'no'" type="string"/>
