            <bind nodeset="/data/part1_4_4/image9" type="binary"/>
            <bind nodeset="/data/part1_4_4/av_functional" type="string"/>
            <bind nodeset="/data/part1_4_4/image10" type="binary"/>
            <bind nodeset="/data/part1_5/hostel" type="string"/>
            <bind nodeset="/data/part1_5/why105" relevant=" /data/part1_5/hostel  = 'no'" type="string"/>
            <bind nodeset="/data/part1_5/image11" type="binary"/>
            <bind nodeset="/data/part1_5/beds" type="int"/>
            <bind nodeset="/data/part1_5/image12" type="binary"/>
            <bind nodeset="/data/part1_5/seats_dining" type="int"/>
            <bind nodeset="/data/part1_5/image13" type="binary"/>
