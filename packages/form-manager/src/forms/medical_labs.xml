                </item>
            </select1>
            <input ref="/data/part1_4_2/comments3">
                <label>Please provide feedback, if any</label>
            </input>
            <upload mediatype="image/*" ref="/data/part1_4_2/image3">
                <label>Please upload image, if any</label>
            </upload>
            <select1 appearance="table-list" ref="/data/part1_4_2/lab_foundation">
                <label>Foundation lab</label>
