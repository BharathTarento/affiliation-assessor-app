    xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <h:head>
        <h:title>Nursing Form- Non Medical (Basic Information)</h:title>
        <model odk:xforms-version="1.0.0">
            <instance>
                <data id="Nursing Form- Non Medical (Basic Information)" version="1">
                    <username />
                    <start />
                    <end />
                    <today />
                    <deviceid />
                    <subscriberid />
                    <part1_1>
                        <note1 />
                        <District />
                        <NameOfIns />
                        <ptcc />
                        <address />
                        <pincode />
                        <email />
                        <gov_priv />
                        <chair_name />
                        <chair_email />
                        <chair_number />
                        <princial_name />
                        <princial_email />
                        <princial_number />
                        <part1_2_1 />
                        <nameofTSMC />
                        <part1_2_2 />
                        <part1_2_2a />
                        <img_part1_2_2a />
                        <url1 />
                        <part1_2_2b />
                        <img_part1_2_2b />
                        <url2 />
                        <part1_2_3 />
                        <part1_2_3a />
                        <img_part1_2_3a />
                        <url3 />
                        <part1_2_3b />
                        <img_part1_2_3b />
                        <url4 />
                    </part1_1>
                    <meta>
                        <instanceID />
                    </meta>
                </data>
            </instance>
            <bind jr:preload="property" jr:preloadParams="username" nodeset="/data/username"
                type="string" />
            <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start"
                type="dateTime" />
            <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime" />
            <bind jr:preload="date" jr:preloadParams="today" nodeset="/data/today" type="date" />
            <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/deviceid"
                type="string" />
            <bind jr:preload="property" jr:preloadParams="subscriberid" nodeset="/data/subscriberid"
                type="string" />
            <bind nodeset="/data/part1_1/note1" readonly="true()" type="string" />
            <bind nodeset="/data/part1_1/District" required="true()" type="string" />
            <bind nodeset="/data/part1_1/NameOfIns" required="true()" type="string" />
            <bind nodeset="/data/part1_1/ptcc" required="true()" type="int" />
            <bind nodeset="/data/part1_1/address" required="true()" type="string" />
            <bind nodeset="/data/part1_1/pincode" required="true()" type="int" />
            <bind nodeset="/data/part1_1/email" required="true()" type="string" />
            <bind nodeset="/data/part1_1/gov_priv" required="true()" type="string" />
            <bind nodeset="/data/part1_1/chair_name" required="true()" type="string" />
            <bind nodeset="/data/part1_1/chair_email" required="true()" type="string" />
            <bind constraint="string-length(.)=10" nodeset="/data/part1_1/chair_number"
                required="true()" type="string" />
            <bind nodeset="/data/part1_1/princial_name" required="true()" type="string" />
            <bind nodeset="/data/part1_1/princial_email" required="true()" type="string" />
            <bind constraint="string-length(.)=10" nodeset="/data/part1_1/princial_number"
                required="true()" type="string" />
            <bind nodeset="/data/part1_1/part1_2_1" readonly="true()" type="string" />
            <bind nodeset="/data/part1_1/nameofTSMC" required="true()" type="string" />
            <bind nodeset="/data/part1_1/part1_2_2" readonly="true()" type="string" />
            <bind nodeset="/data/part1_1/part1_2_2a" required="true()" type="string" />
            <bind nodeset="/data/part1_1/img_part1_2_2a" relevant=" /data/part1_1/part1_2_2a ='yes'"
                required="true()" type="binary" />
            <bind nodeset="/data/part1_1/url1" relevant=" /data/part1_1/part1_2_2a ='yes'"
                type="string" />
            <bind nodeset="/data/part1_1/part1_2_2b" required="true()" type="string" />
            <bind nodeset="/data/part1_1/img_part1_2_2b" relevant=" /data/part1_1/part1_2_2b ='yes'"
                required="true()" type="binary" />
            <bind nodeset="/data/part1_1/url2" relevant=" /data/part1_1/part1_2_2b ='yes'"
                type="string" />
            <bind nodeset="/data/part1_1/part1_2_3" readonly="true()" type="string" />
            <bind nodeset="/data/part1_1/part1_2_3a" required="true()" type="string" />
            <bind nodeset="/data/part1_1/img_part1_2_3a" relevant=" /data/part1_1/part1_2_3a ='yes'"
                required="true()" type="binary" />
            <bind nodeset="/data/part1_1/url3" relevant=" /data/part1_1/part1_2_3a ='yes'"
                type="string" />
            <bind nodeset="/data/part1_1/part1_2_3b" required="true()" type="string" />
            <bind nodeset="/data/part1_1/img_part1_2_3b" relevant=" /data/part1_1/part1_2_3b ='yes'"
                required="true()" type="binary" />
            <bind nodeset="/data/part1_1/url4" relevant=" /data/part1_1/part1_2_3b ='yes'"
                type="string" />
            <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string" />
        </model>
    </h:head>
