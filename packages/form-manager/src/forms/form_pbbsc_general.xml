<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:h="http://www.w3.org/1999/xhtml"
    xmlns:jr="http://openrosa.org/javarosa"
    xmlns:odk="http://www.opendatakit.org/xforms"
    xmlns:orx="http://openrosa.org/xforms"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <h:head>
        <h:title>PBBSc Nuring</h:title>
        <model odk:xforms-version="1.0.0">
            <instance>
                <data id="PBBSc Nuring">
                    <username/>
                    <start/>
                    <end/>
                    <today/>
                    <deviceid/>
                    <subscriberid/>
                    <p_3/>
                    <img_p_3/>
                    <url1/>
                    <p_5>
                        <p_5.1/>
                        <p_5.2/>
                        <p_5.3/>
                        <img_p_3/>
                        <url2/>
                    </p_5>
                    <p_9>
                        <p_9_1/>
                        <p_9_2/>
                        <p_9_3/>
                        <p_9_4/>
                    </p_9>
                    <meta>
                        <instanceID/>
                    </meta>
                </data>
            </instance>
            <bind jr:preload="property" jr:preloadParams="username" nodeset="/data/username" type="string"/>
            <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start" type="dateTime"/>
            <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/>
            <bind jr:preload="date" jr:preloadParams="today" nodeset="/data/today" type="date"/>
            <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/deviceid" type="string"/>
            <bind jr:preload="property" jr:preloadParams="subscriberid" nodeset="/data/subscriberid" type="string"/>
            <bind nodeset="/data/p_3" type="string"/>
            <bind nodeset="/data/img_p_3" relevant=" /data/p_3  = 'yes'" type="binary"/>
            <bind nodeset="/data/url1" relevant=" /data/p_3  = 'yes'" type="string"/>
            <bind nodeset="/data/p_5/p_5.1" type="string"/>
            <bind nodeset="/data/p_5/p_5.2" type="string"/>
            <bind nodeset="/data/p_5/p_5.3" type="string"/>
            <bind nodeset="/data/p_5/img_p_3" relevant=" /data/p_5/p_5.3 ='yes'" type="binary"/>
            <bind nodeset="/data/p_5/url2" relevant=" /data/p_5/p_5.3 ='yes'" type="string"/>
            <bind nodeset="/data/p_9/p_9_1" type="string"/>
            <bind nodeset="/data/p_9/p_9_2" type="string"/>
            <bind nodeset="/data/p_9/p_9_3" type="string"/>
            <bind nodeset="/data/p_9/p_9_4" type="string"/>
            <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
        </model>
    </h:head>
    <h:body>
        <select1 ref="/data/p_3">
            <label>The start and end dates of the academic year included along with the exams and holidays in the academic calender</label>
            <item>
                <label>Yes</label>
                <value>yes</value>
            </item>
            <item>
                <label>No</label>
                <value>no</value>
            </item>
        </select1>
        <upload mediatype="image/*" ref="/data/img_p_3">
            <label>Upload</label>
        </upload>
        <input ref="/data/url1">
            <label>URL Link</label>
        </input>
        <group appearance="field-list" ref="/data/p_5">
            <label>Teaching plan is available with the faculty</label>
            <select1 ref="/data/p_5/p_5.1">
                <label>Teaching plan is developed and the units in the course are distributed to the faculties</label>
                <item>
                    <label>Yes</label>
                    <value>yes</value>
                </item>
                <item>
                    <label>No</label>
                    <value>no</value>
                </item>
            </select1>
            <select1 ref="/data/p_5/p_5.2">
                <label>Faculty has developed a lesson plan</label>
                <item>
                    <label>Yes</label>
                    <value>yes</value>
                </item>
                <item>
                    <label>No</label>
                    <value>no</value>
                </item>
            </select1>
            <select1 ref="/data/p_5/p_5.3">
                <label>Master rotation plan (MRP)/Academic Roaster is displayed - theory, clinical posting, exams, and vacations are mentioned according to the syllabus prescribed</label>
                <item>
                    <label>Yes</label>
                    <value>yes</value>
                </item>
                <item>
                    <label>No</label>
                    <value>no</value>
                </item>
            </select1>
            <upload mediatype="image/*" ref="/data/p_5/img_p_3">
                <label>Upload</label>
            </upload>
            <input ref="/data/p_5/url2">
                <label>URL Link</label>
            </input>
        </group>
        <group appearance="field-list" ref="/data/p_9">
            <label>Effective clinical supervision</label>
            <select1 ref="/data/p_9/p_9_1">
                <label>Faculty record activities conducted during clinical supervision on a daily basis</label>
                <item>
                    <label>Yes</label>
                    <value>yes</value>
                </item>
                <item>
                    <label>No</label>
                    <value>no</value>
                </item>
            </select1>
            <select1 ref="/data/p_9/p_9_2">
                <label>Pre-post clinical discussions are conducted on daily basis</label>
                <item>
                    <label>Yes</label>
                    <value>yes</value>
                </item>
                <item>
                    <label>No</label>
                    <value>no</value>
                </item>
            </select1>
            <select1 ref="/data/p_9/p_9_3">
                <label>Students daily activities are monitored and recorded</label>
                <item>
                    <label>Yes</label>
                    <value>yes</value>
                </item>
                <item>
                    <label>No</label>
                    <value>no</value>
                </item>
            </select1>
            <select1 ref="/data/p_9/p_9_4">
                <label>Periodic clinical evaluations are conducted</label>
                <item>
                    <label>Yes</label>
                    <value>yes</value>
                </item>
                <item>
                    <label>No</label>
                    <value>no</value>
                </item>
            </select1>
        </group>
    </h:body>
</h:html>